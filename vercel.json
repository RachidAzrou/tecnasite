{
  "version": 2,
  "buildCommand": "./build.sh",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["fra1"],
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}